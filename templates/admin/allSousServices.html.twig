{% extends 'base.html.twig' %}

{% block title %}

{% endblock %}


{% block body %}


    <div class="row d-flex justify-content-center text-center">


        <br>
        <br>

        <table class="table table-striped">

            <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Nom</th>
                <th scope="col">Prix</th>

            </tr>
            </thead>

            <tbody>
            {% for sousService in sousServices %}
                <tr>


                    <td>{{ sousService.id }}</td>
                    <td>{{ sousService.nom }}</td>
                    <td>{{ sousService.price }}</td>



                </tr>
            {% endfor %}
            </tbody>
        </table>

    </div>


{% endblock %}